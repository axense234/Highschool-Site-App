{
  "compilerOptions": {
    "sourceMap": false,
    "outDir": "build",
    "strict": false,
    "lib": ["ES5","DOM"],
    "esModuleInterop": true,
    "module": "Node16",
    "moduleResolution": "Node16",
    "skipLibCheck": true,
    "target": "ES5",
    "plugins": [{"transform":"transformer-module"}],
    "baseUrl": "./",
    "paths": {
      "*": ["src/*"]
    }
  },
  "include": ["src/**/*","core/types"],
}